{
	"compilerOptions": {
		"target": "ESNext",
		"lib": ["ESNext"],
		"module": "Preserve",
		"moduleResolution": "Bundler",
		"esModuleInterop": true,
		"strict": true,
		"skipLibCheck": true,
		"noEmit": true,
		"noUncheckedIndexedAccess": true,
		"verbatimModuleSyntax": true
	},
	"include": ["src/**/*.ts"]
}
